<h1>New User</h1>


<%= form_tag(users_url, method: 'post') do %>


  <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>

    <div>
    <%= label_tag :password_digest %><br />
    <%= text_field_tag :password_digest %>
  </div>

    <div>
    <%= label_tag :photo_url %><br />
    <%= text_field_tag :photo_url %>
  </div>

    <div>
    <%= label_tag :latitude %><br />
    <%= text_field_tag :latitude %>
  </div>

    <div>
    <%= label_tag :longitude %><br />
    <%= text_field_tag :longitude %>
  </div>

    <div>
    <%= label_tag :first_name %><br />
    <%= text_field_tag :first_name %>
  </div>

    <div>
    <%= label_tag :last_name %><br />
    <%= text_field_tag :last_name %>
  </div>

  
  <div>
    <%= submit_tag "Create User" %>
  </div>
<% end %>
